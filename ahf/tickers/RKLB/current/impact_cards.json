{
  "revenue_growth_rate": {
    "id": "revenue_growth_rate",
    "inputs": ["Q2_2025_Revenue", "Q3_2025_Revenue_Guidance"],
    "expr": "((Q3_2025_Revenue_Guidance - Q2_2025_Revenue) / Q2_2025_Revenue) * 100",
    "gates": {
      "up": ">=5.0",
      "down": "<0.0"
    }
  },
  "backlog_recognition_rate": {
    "id": "backlog_recognition_rate",
    "inputs": ["Q2_2025_Backlog_12M_Pct"],
    "expr": "Q2_2025_Backlog_12M_Pct",
    "gates": {
      "up": ">=65.0",
      "down": "<50.0"
    }
  },
  "operating_margin_improvement": {
    "id": "operating_margin_improvement",
    "inputs": ["Q2_2025_GAAP_OPM"],
    "expr": "Q2_2025_GAAP_OPM",
    "gates": {
      "up": ">=-20.0",
      "down": "<-50.0"
    }
  }
}